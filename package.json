    },
    "homepage": "https://github.com/Tzukuyomii/api-project#readme",
    "devDependencies": {
        "@tsconfig/node16": "^1.0.3",
        "@types/express": "^4.17.13",
        "@types/jest": "^28.1.4",
        "@types/node": "^18.0.3",
        "@typescript-eslint/eslint-plugin": "^5.30.5",
        "@typescript-eslint/parser": "^5.30.5",
        "concurrently": "^7.2.2",
        "eslint": "^8.19.0",
        "jest": "^28.1.2",
        "nodemon": "^2.0.19",
        "ts-jest": "^28.0.5",
        "typescript": "^4.7.4"
    },
